{
    "ecmaFeatures": {
        "jsx": true
    },
    "env": {
        "browser": true,
        "node": true,
        "amd": true
    },
    "rules": {
        "no-comma-dangle": 0,
        "eol-last": 0,
        "quotes": 0,
        "yoda": 0,
        "no-mixed-requires": 0,
        "key-spacing": 0,
        "no-use-before-define": [2, "nofunc"],
        "eqeqeq": [2, "smart"],
        "no-process-exit": 0,
        "valid-jsdoc": [2, {"requireReturn": false}],
        "complexity": [2, 10],
        "no-else-return": 2,
        "indent": [4, 4],
        "no-lonely-if": 2,
        "no-multiple-empty-lines": [2, {"max": 2}]
    },
    "plugins": [
        "react"
    ]
}
